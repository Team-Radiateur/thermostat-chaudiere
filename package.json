{
	"name": "chaudiere",
	"version": "1.0.0",
	"description": "Le tant attendu bot de la Team Radiateur",
	"main": "build/index.js",
	"scripts": {
		"clean": "rm -rf ./build",
		"compile": "npm run clean && tsc",
		"start": "npm run compile && node build/src/index.js",
		"lint": "eslint ./src --ext ts",
		"lint-fix": "eslint --fix ./src --ext ts",
		"setup": "mkdir ./data && cp ./resources/databases/bot.sqlite ./data/bot.sqlite",
		"prepare": "husky install"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	},
	"keywords": [],
	"author": "Christophe 'DrakeXorn' Bernard",
	"license": "MIT",
	"dependencies": {
		"@discordjs/collection": "1.1.0",
		"@discordjs/opus": "0.8.0",
		"@discordjs/rest": "1.1.0",
		"@discordjs/voice": "0.11.0",
		"axios": "0.27.2",
		"better-sqlite3": "7.6.2",
		"bufferutil": "4.0.6",
		"discord-markdown": "2.5.1",
		"discord-music-player": "9.1.1",
		"discord.js": "14.4.0",
		"dotenv": "16.0.2",
		"erlpack": "0.1.4",
		"express": "4.18.1",
		"ffmpeg-static": "5.1.0",
		"fluent-ffmpeg": "2.1.2",
		"isomorphic-unfetch": "3.1.0",
		"serilogger": "0.4.1",
		"sodium": "3.0.2",
		"string-progressbar": "1.0.4",
		"utf-8-validate": "5.0.9",
		"ytdl-core": "4.11.2",
		"zlib-sync": "0.1.7"
	},
	"devDependencies": {
		"@types/better-sqlite3": "7.6.0",
		"@types/express": "4.17.14",
		"@types/node": "18.7.20",
		"@typescript-eslint/eslint-plugin": "5.38.0",
		"@typescript-eslint/parser": "5.38.1",
		"discord-api-types": "0.37.10",
		"eslint": "8.24.0",
		"husky": "8.0.1",
		"lint-staged": "13.0.3",
		"prettier-eslint": "15.0.1",
		"typescript": "4.8.3"
	}
